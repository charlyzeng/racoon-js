{
  "name": "racoon-js",
  "version": "1.2.2",
  "description": "A tiny tool for checking and cleaning json data.",
  "main": "dist/racoon.umd.js",
  "module": "dist/racoon.esm.js",
  "scripts": {
    "lint": "npx eslint lib test",
    "test": "npx mocha --require @babel/register test/**/*.test.js",
    "cover": "npx nyc --reporter text npm test && npx nyc npm test > /dev/null",
    "build": "npx rollup -c rollup.config.js",
    "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charlyzeng/racoon-js.git"
  },
  "keywords": [
    "check",
    "validate",
    "json",
    "data",
    "check",
    "json",
    "check",
    "data",
    "validate",
    "json",
    "validate",
    "data"
  ],
  "author": "charlyzeng <chenglezeng@tencent.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charlyzeng/racoon-js/issues"
  },
  "homepage": "https://github.com/charlyzeng/racoon-js#readme",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "mocha": "^8.1.0",
    "nyc": "^15.1.0",
    "rollup": "^2.26.2",
    "rollup-plugin-terser": "^7.0.0"
  }
}
